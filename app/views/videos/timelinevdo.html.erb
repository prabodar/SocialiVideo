<h1>Youtube Videos From Timeline Saved to Storage</h1>

<table>
  <thead>
    <tr>
      <th bgcolor="#CDE2FA">Video</th>
    </tr>
  </thead>

  <tbody>

    <% @contents.each do |youtube_vdo| %>




      <div style="width:50%; margin:0 auto;">

        <div id="section">
          <div style="width:500px; height:250px;">

            <%= youtube_vdo%>
            <%= youtube_vdo.html_safe %>

          </div>
        </div>
        <br>

        <div id="footer"><br></div>
      </div>

      </tr>
    <% end %>
  </tbody>
</table>

<br>
